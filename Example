arr = []
print("Enter 10 numbers between 1 to 10:")
for i in range(10):
    num = int(input(f"Enter element {i+1}: "))
    arr.append(num)

print("\nOriginal array:", arr)

reversed_arr = arr[::-1]
print("Reversed array:", reversed_arr)

duplicates = []
for i in arr:
    if arr.count(i) > 1 and i not in duplicates:
        duplicates.append(i)
print("Duplicate elements:", duplicates if duplicates else "no duplicates")

missing = []
for i in range(1, 11):
    if i not in arr:
        missing.append(i)
print("Missing elements:", missing if missing else "no missing elements")
